<div class="row">
  <div class="col-md-10 col-md-offset-1">
    <%= render partial: 'admin_panel' %>
    <%= paginate @task_runs, theme: 'twitter-bootstrap-3' %>
    <table class="table table-striped table-hover table-condensed">
      <tr>
        <th>ID</th>
        <th>Status</th>
        <th>Message</th>
        <th>Language</th>
        <th>Memory</th>
        <th>Time</th>
        <th>Points</th>
        <th>Date</th>
        <th></th>
      </tr>
      <% @task_runs.each do |task_run| %>
        <%= render partial: 'run', locals: { task_run: task_run } %>
      <% end %>
    </table>
    <%= paginate @task_runs, theme: 'twitter-bootstrap-3' %>
  </div>
</div>
