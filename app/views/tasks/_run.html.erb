<tr>
  <td><%= task_run.id %></td>
  <td><%= link_to task_run.task.title, task_path(task_run.task) %></td>
  <td><%= link_to truncate(task_run.user.display_name, length: 15), user_path(task_run.user) if task_run.user.present? %></td>
  <td><%= task_run.status %></td>
  <td><%= task_run.message %></td>
  <td><%= task_run.lang %></td>
  <td><%= task_run.memory_limit_kb %> KB</td>
  <td><%= task_run.time_limit_ms %> ms</td>
  <td><%= task_run.points %></td>
  <td><%= task_run.created_at.to_s(:db) %></td>
  <td><%= render partial: 'task_run_source_code', locals: { task_run: task_run } %></td>
</tr>
